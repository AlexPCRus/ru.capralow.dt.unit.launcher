<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<fragment>
	
	<extension point="com._1c.g5.wiring.serviceProvider">
		<bundle
			symbolicName="ru.capralow.dt.eclemma.core">
		</bundle>
	</extension>
	
	<extension point="org.eclipse.debug.core.launchDelegates">
		<launchDelegate
			delegate="ru.capralow.dt.eclemma.internal.core.ExecutableExtensionFactory:ru.capralow.dt.eclemma.core.launching.BslApplicationLauncher"
			id="com.mountainminds.eclemma.core.bslApplicationLauncher"
			modes="coverage"
			name="%bslDelegateName"
			type="com._1c.g5.v8.dt.launching.core.RuntimeClient"/>
	</extension>
	
	<extension point="org.eclipse.debug.core.launchDelegates">
		<launchDelegate
			delegate="ru.capralow.dt.eclemma.internal.core.ExecutableExtensionFactory:ru.capralow.dt.eclemma.core.launching.BslApplicationLauncher"
			id="com.mountainminds.eclemma.core.UnitTestApplicationLauncher"
			modes="coverage"
			name="%unitTestDelegateName"
			type="ru.capralow.dt.unit.launcher.plugin.ui.UnitTestLaunch"/>
	</extension>
	
</fragment>
